    "react-table": "6.8.6",
    "react-transition-group": "^4.4.1",
    "react-virtualized": "^9.21.2",
    "reactstrap": "^8.4.1",
    "recharts": "^1.8.5",
    "remeda": "^0.0.20",
    "rimraf": "^3.0.2",
    "styled-components": "^5.1.0",
    "typescript": "~3.9.2"
  },
  "resolutions": {
    "graphql": "14.6.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "array-callback-return": "off",
          "eqeqeq": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
